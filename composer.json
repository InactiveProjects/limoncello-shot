{
    "name": "neomerx/limoncello-shot",
    "description": "Quick start JSON API application",
    "keywords": ["framework", "laravel", "lumen", "neomerx", "limoncello", "json-api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "neomerx/limoncello": "~0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "DemoTests\\": "tests/Demo"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
